{
  "name": "myscreen-backend-nest",
  "version": "2.23.0",
  "description": "MyScreen backend API",
  "author": {
    "name": "Stanislav V Vyaliy",
    "email": "wisekaa03@gmail.com",
    "url": "https://wisekaa.dev"
  },
  "homepage": "https://api.myscreen.ru",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "pm2:start": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "test:cov": "jest --config ./jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest.config.js",
    "test:e2e": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.js",
    "docker:prod": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml up --remove-orphans -d",
    "docker:dev": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml up --remove-orphans -d",
    "docker:prod:down": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml down --rmi all -v",
    "docker:dev:down": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml down --rmi all -v",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "resolutions": {
    "**/**/@elastic/elasticsearch": ">=8.2.1",
    "**/**/body-parser": ">=1.20.0",
    "**/**/concat-stream": ">=2.0.0",
    "**/**/debug": ">=4.3.4",
    "**/**/dotenv": ">=16.0.1",
    "**/**/execa": "^5.1.1",
    "**/**/express": ">=4.18.1",
    "**/**/fabric": ">=5.2.1",
    "**/**/form-data": ">=4.0.0",
    "**/**/gl": ">=5.0.3",
    "**/**/inquirer": "^8.2.4",
    "**/**/magic-string": ">=0.26.2",
    "**/**/minimatch": ">=5.0.1",
    "**/**/multer": ">=1.4.5-lts.1",
    "**/**/node-gyp": ">=9.1.0",
    "**/**/npmlog": ">=6.0.2",
    "**/**/on-finished": ">=2.4.1",
    "**/**/optionator": ">=0.9.1",
    "**/**/readable-stream": ">=4.1.0",
    "**/**/pino": ">=8.4.0",
    "**/**/send": ">=0.18.0",
    "**/**/serve-static": ">=1.15.0",
    "**/**/split2": ">=4.1.0",
    "**/**/swagger-ui-dist": ">=4.13.2",
    "**/**/through2": ">=4.0.2",
    "**/**/tr46": ">=3.0.0",
    "**/**/tslib": ">=2.4.0",
    "**/**/typescript": ">=4.7.4",
    "**/**/qs": ">=6.11.0",
    "**/**/url": ">=0.11.0",
    "**/**/uuid": ">=8.3.2",
    "**/**/ws": ">=8.8.1",
    "**/**/xml2js": ">=0.4.23"
  },
