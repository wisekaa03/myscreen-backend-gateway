{
  "name": "myscreen-backend-nest",
  "version": "2.0.8",
  "description": "MyScreen backend API docs",
  "author": "Stanislav V Vyaliy",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:prod": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml up --remove-orphans -d",
    "docker:dev": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml up --remove-orphans -d",
    "docker:prod:down": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml down --rmi all -v",
    "docker:dev:down": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml down --rmi all -v"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "**/**/async": "^3",
    "**/**/debug": "^4",
    "**/**/dotenv": "^10",
    "**/**/typescript": "^4",
    "**/**/webpack": "^5",
    "**/**/ws": "^8"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/platform-socket.io": "^8.2.3",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.3",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-safe-stringify": "^2.1.1",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.2",
    "jsonwebtoken": "^8.5.1",
    "nest-winston": "^1.6.2",
    "nestjs-mailgun": "^2.0.11",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-native": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.2.0",
    "typeorm": "^0.2.41",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cli-color": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mailgun-js": "^0.22.12",
    "@types/node": "^16.11.11",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
