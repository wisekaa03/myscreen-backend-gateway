{
  "name": "myscreen-backend-nest",
  "version": "2.52.1",
  "description": "MyScreen backend API",
  "author": {
    "name": "Stanislav V Lanskoy",
    "email": "stanislav@wisekaa.dev",
    "url": "https://wisekaa.dev"
  },
  "homepage": "https://api.myscreen.ru",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "pm2:start": "pm2 restart ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "build": "NODE_ENV=production nest build",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules ~/.npm/_cacache ~/.cache/yarn",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint . --fix --ignore-pattern \"dist/\"",
    "test": "jest --config ./jest.config.cjs",
    "test:watch": "jest --config ./jest.config.cjs --watch",
    "test:cov": "jest --config ./jest.config.cjs --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest.config.cjs",
    "test:e2e": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.cjs",
    "test:e2e:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles --config ./test/jest-e2e.cjs",
    "docker:prod": "sudo docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml up --remove-orphans -d",
    "docker:dev": "sudo docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml up --remove-orphans -d",
    "docker:prod:down": "sudo docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml down --rmi all -v",
    "docker:dev:down": "sudo docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml down --rmi all -v",
    "prepare": "husky install"
  },
