{
  "name": "myscreen-backend-nest",
  "version": "2.8.2",
  "description": "MyScreen backend API",
  "author": {
    "name": "Stanislav V Vyaliy",
    "email": "wisekaa03@gmail.com",
    "url": "https://wisekaa.dev"
  },
  "homepage": "https://api.myscreen.ru",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "pm2-start": "pm2 start --name \"api\" \"xvfb-run yarn start:prod\"",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "test:cov": "jest --config ./jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest.config.js",
    "test:e2e": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.js",
    "docker:prod": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml up --remove-orphans -d",
    "docker:dev": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml up --remove-orphans -d",
    "docker:prod:down": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml down --rmi all -v",
    "docker:dev:down": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml down --rmi all -v",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.18.3"
  },
  "resolutions": {
    "**/**/chokidar": ">=3.5.3",
    "**/**/debug": ">=4.3.3",
    "**/**/dotenv": ">=15.0.0",
    "**/**/execa": "^5.1.1",
    "**/**/express": ">=4.17.2",
    "**/**/gl": ">=5.0.0",
    "**/**/inquirer": ">=8.2.0",
    "**/**/ky": ">=0.25.1",
    "**/**/multer": ">=1.4.4",
    "**/ky-universal/**/node-fetch": ">=3.1.0",
    "**/**/schema-utils": ">=4.0.0",
    "**/**/source-map-support": ">=0.5.21",
    "**/**/source-map": ">=0.7.3",
    "**/**/tr46": ">=3.0.0",
    "**/**/tslib": ">=2.3.1",
    "**/**/typescript": ">=4.5.5",
    "**/**/url": ">=0.11.0",
    "**/**/ws": ">=8.4.2",
    "**/**/xml2js": ">=0.4.23"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-ws": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.2.6",
    "aws-sdk": "^2.1066.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "debug": "^4.3.3",
    "dotenv": "^15.0.0",
    "editly": "^0.11.3",
    "express": "^4.17.2",
    "ioredis": "^4.28.3",
    "jsonwebtoken": "^8.5.1",
    "media-probe": "^1.0.1",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "multer-media": "^2.1.1",
    "nestjs-mailgun": "^2.0.18",
    "nestjs-pino": "^2.5.0",
    "nestjs-s3": "^1.0.1",
    "node-fetch": "^3.2.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-native": "^3.0.0",
    "pino": "^7.6.5",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "ws": "^8.4.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
