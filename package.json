{
  "name": "myscreen-backend-nest",
  "version": "2.3.3",
  "description": "MyScreen backend API",
  "author": {
    "name": "Stanislav V Vyaliy",
    "email": "wisekaa03@gmail.com",
    "url": "https://wisekaa.dev"
  },
  "homepage": "https://api.myscreen.ru",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --config ./jest.config.js --watch",
    "test:cov": "jest --config ./jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest.config.js",
    "test:e2e": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.js",
    "docker:prod": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml up --remove-orphans -d",
    "docker:dev": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml up --remove-orphans -d",
    "docker:prod:down": "docker-compose -p myscreen-prod -f docker/docker-compose-prod.yml down --rmi all -v",
    "docker:dev:down": "docker-compose -p myscreen-dev -f docker/docker-compose-dev.yml down --rmi all -v",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "**/**/debug": "^4.3.3",
    "**/**/multer": "^1.4.4",
    "**/**/node-fetch": "^3.1.0",
    "**/**/ky": ">=0.25.1",
    "**/**/source-map-support": ">=0.5.21",
    "**/**/source-map": ">=0.7.3",
    "**/**/tslib": "^2.3.1",
    "**/**/typescript": "^4.5.4",
    "**/**/url": ">=0.11.0",
    "**/**/ws": "^8.3.0"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.2.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "aws-sdk": "^2.1046.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "ioredis": "^4.28.2",
    "jsonwebtoken": "^8.5.1",
    "media-probe": "^1.0.1",
    "multer": "^1.4.4",
    "multer-media": "^2.1.0",
    "nest-winston": "^1.6.2",
    "nestjs-mailgun": "^2.0.13",
    "nestjs-s3": "^1.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-native": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "winston": "^3.3.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.1",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "depcheck": "^1.4.2",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
